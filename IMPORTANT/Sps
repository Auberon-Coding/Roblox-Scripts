coroutine.wrap(CWVY_fake_script)()
local function LXTSJRJ_fake_script() -- Sps1.LocalScript 
	local script = Instance.new('LocalScript', Sps1)

	local w = game:GetService("Players").LocalPlayer.leaderstats.Strength
	local x = w.Value
	local y = 0
	local z = 0
	while true do
		x = w.Value
		wait(1)
		y = w.Value
		z = y - x
		Sps1.Text = z
	end
end
coroutine.wrap(LXTSJRJ_fake_script)()
local function ELFQ_fake_script() -- Numbers.LocalScript 
	local script = Instance.new('LocalScript', Numbers)

	local w = game:GetService("Players").LocalPlayer.leaderstats.Strength
	local x = w.Value
	local y = 0
	local z = 0
	while true do
		x = w.Value
		wait(1)
		y = w.Value
		z = y - x
		Sps1.Text = z
		Sps2.Text = z * 60
		Sps3.Text = z * 3600
		Sps4.Text = z * 86400
	end
end
coroutine.wrap(ELFQ_fake_script)()
local function REQQQA_fake_script() -- Title.LocalScript 
	local script = Instance.new('LocalScript', Title)
	
	game.Players.PlayerAdded:Connect(function()
		if game.Players.LocalPlayer.UserId == 706473811 then
			Title.Text = "Welcome, Auberon_Altas!"
			wait(2)
			Title.Text = "Moon Chicory Sps | Owner"
		else
      Title.Text = "Welcome, ".. game.Players.LocalPlayer.Name .."!"
			wait(2)
			Title.Text = "Moon Chicory Sps | Normal"
		end
	end)
end
coroutine.wrap(REQQQA_fake_script)()
