-- Scripts:

local function IMXTEGP_fake_script() -- Sps4.LocalScript 
	local script = Instance.new('LocalScript', Sps4)

	local w = game:GetService("Players").LocalPlayer.leaderstats.Strength
	local x = w.Value
	local y = 0
	local z = 0
	while true do
		x = w.Value
		wait(86400)
		y = w.Value
		z = y - x
		Sps4.Text = z
	end
end
coroutine.wrap(IMXTEGP_fake_script)()
local function FTVHL_fake_script() -- Sps3.LocalScript 
	local script = Instance.new('LocalScript', Sps3)

	local w = game:GetService("Players").LocalPlayer.leaderstats.Strength
	local x = w.Value
	local y = 0
	local z = 0
	while true do
		x = w.Value
		wait(3600)
		y = w.Value
		z = y - x
		Sps3.Text = z
	end
end
coroutine.wrap(FTVHL_fake_script)()
