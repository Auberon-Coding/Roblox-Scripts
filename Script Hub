local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Script Hub", "DarkTheme")

local ITab = Window:NewTab("Info")
local ISection = ITab:NewSection("Info")
ISection:NewLabel("Thank You For Using My Script-Hub!")
ISection:NewLabel("Note: MOST OF THIS SCRIPTS ARE NOT MINE")
ISection:NewLabel("Note: I'm Not Responsible For Your Ban")
ISection:NewLabel("This Is Made By Auberon_Altas")
ISection:NewKeybind("If You Press E It Will Close The Script", "", Enum.KeyCode.E, function()
	Library:ToggleUI()
end)

local MTab = Window:NewTab("Mining Clicker")
local MSection = MTab:NewSection("Script(s)")
MSection:NewButton("MiningScript", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dady172172/Roblox-Cheat-Scripts/master/Mining%20Clicker%20Simulator%20GUI.lua"))()
end)

local STab = Window:NewTab("SPTS")
local SSection = STab:NewSection("Script(s)")
SSection:NewButton("Spts Uni", "", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/D9btNZUr', true))()
end)

SSection:NewButton("Spts Custom", "", function()
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/erikkire123/New-Spts-script/main/newsptscscript.txt"), true))()
end)

local BTab = Window:NewTab("BedWars")
local BSection = BTab:NewSection("Script(s)")
BSection:NewButton("BedWars Private", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vodxn/sape/main/Initiate.lua"))()
end)

local Tab = Window:NewTab("Multi Purpose")
local Section = Tab:NewSection("Script(s)")
Section:NewButton("Anti Afk", "", function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)

Section:NewButton("Auto-Clicker", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/JustEzpi/ROBLOX-Scripts/main/ROBLOX_AutoClicker"))()
end)

Section:NewButton("Invisible Tool", "", function()
    loadstring(game:HttpGet("https://gist.githubusercontent.com/skid123skidlol/cd0d2dce51b3f20ad1aac941da06a1a1/raw/f58b98cce7d51e53ade94e7bb460e4f24fb7e0ff/%257BFE%257D%2520Invisible%2520Tool%2520(can%2520hold%2520tools)",true))()
end)
